<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <font>Tahoma</font>
  <start_on_axis>0</start_on_axis>
  <redraw>true</redraw>
  <plot_area>
    <border_alpha>100</border_alpha>
    <margins>
      <left>60</left>
      <right>60</right>
      <top>20</top>
      <bottom>100</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>10</alpha>
      <approx_count>12</approx_count>
    </x>
    <y_right>
      <enabled>0</enabled>
    </y_right>
  </grid>
  <digits_after_decimal>2</digits_after_decimal>
  <decimals_separator>.</decimals_separator>
  <thousands_separator>,</thousands_separator>
  <context_menu>
    <menu mid="4" function_name="showAll" title="전체보기"/>
  </context_menu>
  <values>
    <x>
      <inside>false</inside>
    </x>
    <y_left>
      <inside>false</inside>
    </y_left>
    <y_right>
      <inside>false</inside>
    </y_right>
  </values>
