            <Border x:Name="Background_Invalid" Background="{StaticResource ItemBackground}" Margin="1,1,2,2" BorderThickness="1" CornerRadius="1" BorderBrush="{StaticResource ControlOuterBorder_Invalid}" Grid.Column="2" Grid.ColumnSpan="2" Visibility="Collapsed">
                <ToolTipService.ToolTip>
