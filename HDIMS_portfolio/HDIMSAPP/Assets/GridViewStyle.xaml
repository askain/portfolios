                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="00:00:00.2000000" To="Normal" />
                        <VisualTransition GeneratedDuration="00:00:00.2000000" To="MouseOver" />
